LAYER
    NAME "sd_detections"
    TYPE POINT
    STATUS ON
    PROJECTION "init=epsg:${SRID}" END
    EXTENT ${MAPFILE_EXTENT}
    TEMPLATE fooOnlyForWMSGetFeatureInfo
    CONNECTIONTYPE POSTGIS
    PROCESSING "CLOSE_CONNECTION=DEFER"
    CONNECTION "user=${PGUSER} password=${PGPASSWORD} dbname=${PGDATABASE} host=${PGHOST}"
    DATA "geom FROM (SELECT geo.geom, geo.id,
                            geo.numero, geo.feu, geo.gaz, geo.sprinkler, geo.localisation, geo.adresse, geo.photo AS photo_empty, geo.remarque
                     FROM pompiers.sd_vgeo_detections AS geo
                     WHERE (ARRAY[%role_ids%] && ARRAY(${MAPSERVER_DATA_NOAREA_SUBSELECT} 'sd_detections'))
                    ) AS foo USING UNIQUE id USING srid=${SRID}"
    TOLERANCE ${WMS_TOLERANCE}
    DUMP TRUE
    METADATA
        "wms_title" "Détections"
        "ows_geom_type" "point"
        "gml_featureid" "id"
        "gml_types" "auto"
        "gml_include_items" "all"
        "gml_exclude_items" "id"
        "wms_srs" "${WMS_SRS}"
        "wfs_enable_request" "*"
        "ows_geometries" "geom"
    END
    VALIDATION
        "default_role_ids" "-1"
        "role_ids" "^-?[0-9,]*$"
    END
    MINSCALEDENOM 1
    MAXSCALEDENOM 901
    LABELITEM "localisation"
    CLASS
        NAME "Détections"
        STYLE
            SYMBOL "sd_detection"
            SIZE 20
        END
        LABEL
            TYPE TRUETYPE
            FONT "OpenSans-Bold"
            SIZE 8
            ANTIALIAS TRUE
            COLOR 255 255 255
            OUTLINECOLOR 255 0 0
            OUTLINEWIDTH 1
            POSITION LC
            ALIGN CENTER
            OFFSET 0 5
            WRAP " "
            MAXLENGTH 15
            FORCE TRUE
        END
    END
END

